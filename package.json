{
	"name": "@ctx-core/package",
	"version": "9.1.21",
	"description": "ctx-core package",
	"keywords": [
		"ctx-core",
		"package"
	],
	"homepage": "https://github.com/ctx-core/package#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/package/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/package.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./src/index.d.ts",
	"exports": {
		".": {
			"browser": "./src/index.browser.js",
			"node": "./src/index.js"
		},
		"./package.json": "./package.json"
	},
	"bin": {
		"verify-node-version": "bin/verify-node-version.mjs",
		"verify-version-node.js": "bin/verify-node-version.mjs"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build && npm run test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsm node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@ctx-core/cli-args": "workspace:^11.0.20",
		"import-meta-resolve": "^1.1.1",
		"semver": "^7.3.5"
	},
	"devDependencies": {
		"@types/node": "^17.0.6",
		"@types/resolve": "^1.20.1",
		"@types/semver": "^7.3.9",
		"c8": "^7.11.0",
		"check-dts": "^0.6.6",
		"resolve": "^1.20.0",
		"rimraf": "^3.0.2",
		"tsm": "^2.2.1",
		"typescript": "next",
		"uvu": "^0.5.2"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./src/index.js"
}
