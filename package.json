{
	"name": "@ctx-core/package",
	"version": "9.2.7",
	"description": "ctx-core package",
	"keywords": [
		"ctx-core",
		"package"
	],
	"homepage": "https://github.com/ctx-core/package#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/package/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/package.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./src/index.d.ts",
	"exports": {
		".": {
			"browser": "./src/index.browser.js",
			"default": "./src/index.js",
			"node": "./src/index.js"
		},
		"./package.json": "./package.json"
	},
	"bin": {
		"verify-node-version": "bin/verify-node-version.mjs",
		"verify-version-node.js": "bin/verify-node-version.mjs"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build && npm run test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@ctx-core/cli-args": "^11.1.7",
		"import-meta-resolve": "^2.2.1",
		"semver": "^7.3.8"
	},
	"devDependencies": {
		"@types/node": "^18.14.0",
		"@types/resolve": "^1.20.2",
		"@types/semver": "^7.3.13",
		"c8": "^7.13.0",
		"check-dts": "^0.7.0",
		"resolve": "^1.22.1",
		"rimraf": "^4.1.2",
		"tsx": "^3.12.3",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./src/index.js",
	"sideEffects": false
}