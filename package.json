{
	"name": "@ctx-core/package",
	"version": "9.1.20",
	"description": "ctx-core package",
	"keywords": [
		"ctx-core",
		"package"
	],
	"homepage": "https://github.com/ctx-core/package#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/package/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/package.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"browser": "./dist/index.browser.js",
			"node": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"bin": {
		"verify-node-version": "bin/verify-node-version.mjs",
		"verify-version-node.js": "bin/verify-node-version.mjs"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf dist && npm run clean_tsbuildinfo",
		"clean_tsbuildinfo": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.d.ts",
		"compile": "npm run compile_source && npm run compile_declaration",
		"compile_source": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"compile_declaration": "npm run clean_tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir dist",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/cli-args": "workspace:^11.0.19",
		"import-meta-resolve": "^1.1.1",
		"semver": "^7.3.5"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.121",
		"@types/node": "^17.0.2",
		"@types/resolve": "^1.20.1",
		"@types/semver": "^7.3.9",
		"resolve": "^1.20.0",
		"rimraf": "^3.0.2",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./dist/index.js"
}