{
	"name": "@ctx-core/package",
	"version": "9.1.7",
	"description": "ctx-core package",
	"keywords": [
		"ctx-core",
		"package"
	],
	"homepage": "https://github.com/ctx-core/package#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/package/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/package/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"bin": {
		"verify-node-version": "bin/verify-node-version.mjs",
		"verify-version-node.js": "bin/verify-node-version.mjs"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/cli-args": "workspace:^11.0.8",
		"import-meta-resolve": "^1.1.1",
		"semver": "^7.3.5"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.52",
		"@swc/core": "^1.2.118",
		"@types/node": "^16.11.11",
		"@types/resolve": "^1.20.1",
		"@types/semver": "^7.3.9",
		"resolve": "^1.20.0",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}