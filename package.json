{
	"name": "@ctx-core/package",
	"version": "9.3.82",
	"description": "ctx-core package",
	"keywords": [
		"ctx-core",
		"package"
	],
	"homepage": "https://github.com/ctx-core/package#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/package/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/package.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./bin",
		"./_types",
		"./engines_node_",
		"./node_version__verify",
		"./package_json_",
		"./package_version_"
	],
	"types": "./index.d.ts",
	"exports": {
		".": {
			"browser": "./index.browser.js",
			"default": "./index.js",
			"node": "./index.js"
		},
		"./package.json": "./package.json"
	},
	"bin": {
		"verify-node-version": "bin/verify-node-version.mjs",
		"verify-version-node.js": "bin/verify-node-version.mjs"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test:unit && pnpm test:types",
		"test:types": "check-dts",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test:unit:coverage": "c8 pnpm test:unit"
	},
	"dependencies": {
		"ctx-core": "^5.16.1",
		"import-meta-resolve": "^4.0.0",
		"semver": "^7.5.4"
	},
	"devDependencies": {
		"@types/node": "^20.10.7",
		"@types/resolve": "^1.20.6",
		"@types/semver": "^7.5.6",
		"c8": "^9.0.0",
		"check-dts": "^0.7.2",
		"resolve": "^1.22.8",
		"tsx": "^4.7.0",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}