{
	"name": "@ctx-core/package",
	"version": "9.3.211",
	"description": "ctx-core package",
	"keywords": [
		"ctx-core",
		"package"
	],
	"homepage": "https://github.com/ctx-core/package#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/package/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/package.git"
	},
	"license": "Apache-2.0",
	"author": {
		"name": "Brian Takita",
		"url": "https://briantakita.me",
		"email": "info+ctx-core-package@briantakita.me"
	},
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./bin",
		"./_types",
		"engines_node",
		"./node_version__verify",
		"package_json",
		"package_version"
	],
	"types": "./index.d.ts",
	"exports": {
		".": {
			"browser": "./index.browser.js",
			"default": "./index.js",
			"node": "./index.js"
		},
		"./package.json": "./package.json"
	},
	"bin": {
		"verify-node-version": "bin/verify-node-version.mjs",
		"verify-version-node.js": "bin/verify-node-version.mjs"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm run /^test:/",
		"test:types": "check-dts",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"disable:test:unit:coverage": "c8 pnpm test:unit"
	},
	"dependencies": {
		"ctx-core": "^7.2.18",
		"import-meta-resolve": "^4.1.0",
		"semver": "^7.6.2"
	},
	"devDependencies": {
		"@types/node": "^20.14.2",
		"@types/resolve": "^1.20.6",
		"@types/semver": "^7.5.8",
		"c8": "^10.0.0",
		"check-dts": "^0.8.0",
		"resolve": "^1.22.8",
		"tsx": "^4.15.1",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": false
}