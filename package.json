{
	"name": "@ctx-core/package",
	"version": "9.0.15",
	"description": "ctx-core package",
	"keywords": [
		"ctx-core",
		"package"
	],
	"homepage": "https://github.com/ctx-core/package#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/package/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/package/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"bin": {
		"verify-version-node.js": "bin/verify-version-node.mjs"
	},
	"scripts": {
		"build": "npm run clean && npm run copy-mjs && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"copy-mjs": "cd dist && echo \"module.exports = (async () => import('./index.js'))()\" > index.cjs",
		"exec": "$@",
		"prepublishOnly": "npm run build"
	},
	"dependencies": {
		"@ctx-core/cli-args": "workspace:^10.0.18",
		"esm": "^3.2.25",
		"import-meta-resolve": "^1.1.1"
	},
	"devDependencies": {
		"@types/resolve": "^1.20.1",
		"resolve": "^1.20.0",
		"typescript": "^4.4.2"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}